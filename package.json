{
  "name": "dynamodb-loader",
  "version": "1.0.0",
  "description": "Loading JSON data to DynamoDB the simplest and consistent way!",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "convert:employee": "node csv2json.js -i data/2022-03-08_InnoEmployee_R1.csv -o data/2022-03-08_InnoEmployee_R1.json -t -p -n Employee",
    "convert:position": "node csv2json.js -i data/2022-03-09_InnoPosition_R1.csv -o data/2022-03-09_InnoPosition_R1.json -t -p -n Position",
    "convert:department": "node csv2json.js -i data/2022-03-09_InnoDepartment_R1.csv -o data/2022-03-09_InnoDepartment_R1.json -t -p -n Department",
    "push:employee": "node main.js -i data/2022-03-08_InnoEmployee_R1.json",
    "push:position": "node main.js -i data/2022-03-09_InnoPosition_R1.json",
    "push:department": "node main.js -i data/2022-03-09_InnoDepartment_R1.json",
    "all:employee": "npm run convert:employee && npm run push:employee",
    "all:position": "npm run convert:position && npm run push:position",
    "all:department": "npm run convert:department && npm run push:department",
    "all": "npm-run-all all:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/datability-th/dynamodb-loader.git"
  },
  "author": "Thanapapas Horsuwan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/datability-th/dynamodb-loader/issues"
  },
  "homepage": "https://github.com/datability-th/dynamodb-loader#readme",
  "dependencies": {
    "aws-sdk": "^2.1081.0",
    "csv-parse": "^5.0.4",
    "dotenv": "^16.0.0",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
