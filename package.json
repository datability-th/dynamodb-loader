    "push:position": "node main.js -i data/2022-03-09_InnoPosition_R1.json",
    "push:department": "node main.js -i data/2022-03-09_InnoDepartment_R1.json",
    "all:employee": "npm run convert:employee && npm run push:employee",
    "all:position": "npm run convert:position && npm run push:position",
    "all:department": "npm run convert:department && npm run push:department",
    "all": "npm-run-all all:*",
